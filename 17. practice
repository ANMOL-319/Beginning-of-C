#include <stdio.h>
int main(){
    int a, b, c;
    FILE *ptr;
    ptr = fopen("read.txt", "r");
    fscanf(ptr, "%d %d %d", &a, &b, &c);
    

    printf("The three integers from the file is %d, %d, %d\n", a, b, c);
   

    return 0;}

// NOTE: we have to make a separate 'read.txt' file to successfully run the program
