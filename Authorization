#include <stdio.h>
int main()
{
    int age;
    printf("Enter your age :");
    scanf("%d", &age);

    if (age >= 90)//>= means greater or equal to
   //  (age!=90) // != means not equal to
    {
        printf("Your age is above 90, you cannot drive\n");
    }
    else{
         printf("Your age is lower than 90, you can drive\n");
    }

    return 0;
}
