#include <stdio.h>
int sum (int a, int b);
int main(){
    int a, b;
    a=2, b=7;

    printf("The sum of 2+7 is %d\n", sum(2,7));
    printf("The value of a + b is %d\n ", a + b);
    printf("The value of a and b is %d and %d\n", a, b);
    return 0;}

    int sum (int a, int b){
    int c;
    c = a + b;

    a=789;
    b=985;
    return c;
    }
