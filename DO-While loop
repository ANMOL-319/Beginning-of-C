#include <stdio.h>
int main()
{
    int i = 0;

    /*Do-While loop= This type of loop executes at least once.*/

    do
    {
        printf("The value of i is %d\n", i);
        i++;
    } while (i <= 10);

    return 0;}

    // While loop= It checks the condition and then executes the code
    // Do-While loop= It executes the code and then checks the condition 
